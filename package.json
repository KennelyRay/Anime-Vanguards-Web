{
  "name": "anime-vanguards-web",
  "version": "1.0.0",
  "type": "module",
  "description": "Fanmade webapp for Anime Vanguards - Unit guides, tier lists, and game information",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "server": "cd server && node app.js",
    "server:dev": "cd server && nodemon app.js",
    "seed": "cd server && node seed.js",
    "start": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start:prod": "concurrently \"npm run server\" \"npm run preview\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^4.18.2",
    "framer-motion": "^10.12.16",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "mongoose": "^8.16.1",
    "multer": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "keywords": [
    "anime-vanguards",
    "roblox",
    "gaming",
    "fansite",
    "react"
  ],
  "author": "Anime Vanguards Community",
  "license": "MIT"
}
 